<package type="addon" name="nuxeo-elastic-search" version="@VERSION@">
  <title>Nuxeo Elastic Search Integration</title>
  <description>
    <p>Add support for Elasticsearch based indexing and query</p>
  </description>
  <home-page>https://github.com/nuxeo/nuxeo-elastic-search</home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>testing</production-state>
  <supported>true</supported>
  <platforms>
    <platform>cap-@DISTRIB_VERSION@</platform>
  </platforms>
  <license>LGPL 2.1</license>
  <license-url>http://www.gnu.org/licenses/lgpl-2.1.html</license-url>
</package>
